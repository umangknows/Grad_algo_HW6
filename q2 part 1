import random
import matplotlib.pyplot as plt
import numpy as np



d = dict()

for i in range(100000):
    d[i]=0

# print(len(d))
d2 = dict(d)
keys = list(d.keys())

for i in range(100000):
    r = random.choice(keys)
    d[r]+=1
values = d.values()
plt.hist(values, bins = [0,1,2,3,4,5, 6, 7, 8])
plt.show()
